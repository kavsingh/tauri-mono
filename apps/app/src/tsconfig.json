{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../../../tsconfig.base.json",
	"compilerOptions": {
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"lib": ["esnext", "dom"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"paths": { "#*": ["./*"] },
		"sourceMap": true,
		"target": "esnext"
	}
}
