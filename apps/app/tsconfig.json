{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"lib": ["esnext", "dom"],
		"noEmit": true,
		"paths": { "#*": ["./src/*"] },
		"sourceMap": true
	},
	"exclude": ["**/node_modules"],
	"include": ["./src", "./scripts", "./*.ts", "./*.cjs", "./*.*.cjs"]
}
