{
	"name": "tauri-mono",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "git@github.com:kavsingh/tauri-mono"
	},
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
	"scripts": {
		"dev": "turbo dev",
		"test": "turbo test",
		"test:ci": "turbo test:ci",
		"lint": "turbo lint",
		"build:mac": "turbo build:mac --filter=app",
		"build:win": "turbo build:win --filter=app",
		"clean:turbo": "rm -rf .turbo && pnpm -r exec rm -rf .turbo",
		"clean:modules": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
		"clean:types": "pnpm -r exec rm -f tsconfig.tsbuildinfo && pnpm -r exec rm -f tsconfig.ci.tsbuildinfo"
	},
	"devDependencies": {
		"@eslint/js": "9.34.0",
		"@kavsingh/eslint-plugin-filenames": "2.0.0-rc.2",
		"@tsconfig/node24": "24.0.1",
		"@tsconfig/strictest": "2.0.5",
		"@types/eslint": "9.6.1",
		"@types/node": "24.3.0",
		"eslint": "9.34.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-prettier": "5.5.4",
		"globals": "16.3.0",
		"prettier": "3.6.2",
		"rimraf": "6.0.1",
		"turbo": "2.5.6",
		"typescript": "5.9.2",
		"typescript-eslint": "8.42.0"
	}
