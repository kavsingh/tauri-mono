	"repository": {
		"type": "git",
		"url": "git@host.com:your/repo"
	},
	"type": "module",
	"scripts": {
		"dev": "nx dev app --outputStyle stream",
		"test": "nx run-many -t test --outputStyle stream",
		"test:ci": "nx run-many -t test:ci --outputStyle stream",
		"lint": "conc 'nx run-many -t lint --outputStyle stream' 'tombi lint --error-on-warnings'",
		"build:mac": "nx build:mac app",
		"build:win": "nx build:win app",
		"clean:nx": "pnpm -r exec rm -rf .nx && rm -rf .nx",
		"clean:modules": "pnpm -r exec rm -rf node_modules && rm -rf node_modules",
		"clean:targets": "pnpm -r exec rm -rf target && rm -rf target"
	},
	"devDependencies": {
		"@tsconfig/node24": "24.0.4",
		"@tsconfig/strictest": "2.0.8",
		"@types/node": "24.10.9",
		"@typescript/native-preview": "7.0.0-dev.20260128.1",
		"concurrently": "9.2.1",
		"lefthook": "2.0.16",
		"nx": "22.4.2",
		"oxfmt": "0.27.0",
		"oxlint": "1.42.0",
		"oxlint-tsgolint": "0.11.3",
		"rimraf": "6.1.2",
		"tombi": "0.7.25",
		"typescript": "5.9.3"
	},
	"packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
