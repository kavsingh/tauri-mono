{
	"name": "tauri-mono",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "git@github.com:kavsingh/tauri-mono"
	},
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
	"scripts": {
		"dev": "turbo dev",
		"test": "turbo test",
		"test:ci": "turbo test:ci",
		"lint": "turbo lint",
		"build:mac": "turbo build:mac --filter=app",
		"build:win": "turbo build:win --filter=app",
		"clean:turbo": "rm -rf .turbo && pnpm -r exec rm -rf .turbo",
		"clean:modules": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
		"clean:types": "pnpm -r exec rm -f tsconfig.tsbuildinfo && pnpm -r exec rm -f tsconfig.ci.tsbuildinfo"
	},
	"devDependencies": {
		"@eslint/js": "9.36.0",
		"@kavsingh/eslint-plugin-filenames": "2.0.0-rc.3",
		"@tsconfig/node24": "24.0.1",
		"@tsconfig/strictest": "2.0.6",
		"@types/eslint": "9.6.1",
		"@types/node": "24.6.0",
		"eslint": "9.36.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-prettier": "5.5.4",
		"globals": "16.4.0",
		"prettier": "3.6.2",
		"rimraf": "6.0.1",
		"turbo": "2.5.8",
		"typescript": "5.9.2",
		"typescript-eslint": "8.45.0"
	}
}
