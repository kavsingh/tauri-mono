{
	"name": "tauri-mono",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "git@github.com:kavsingh/tauri-mono"
	},
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
	"scripts": {
		"dev": "turbo dev",
		"test": "turbo test",
		"test:ci": "turbo test:ci",
		"lint": "turbo lint",
		"build:mac": "turbo build:mac --filter=app",
		"build:win": "turbo build:win --filter=app",
		"clean:turbo": "rm -rf .turbo && pnpm -r exec rm -rf .turbo",
		"clean:modules": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
		"clean:types": "pnpm -r exec rm -f tsconfig.tsbuildinfo && pnpm -r exec rm -f tsconfig.ci.tsbuildinfo"
	},
	"devDependencies": {
		"@eslint/js": "9.29.0",
		"@kavsingh/eslint-plugin-filenames": "2.0.0-rc.1",
		"@tsconfig/node24": "24.0.1",
		"@tsconfig/strictest": "2.0.5",
		"@types/eslint": "9.6.1",
		"@types/node": "24.0.3",
		"eslint": "9.29.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-import-resolver-typescript": "4.4.3",
		"eslint-plugin-import-x": "4.15.2",
		"eslint-plugin-prettier": "5.4.1",
		"globals": "16.2.0",
		"prettier": "3.5.3",
		"rimraf": "6.0.1",
		"turbo": "2.5.4",
		"typescript": "5.8.3",
		"typescript-eslint": "8.34.0"
	}
}
