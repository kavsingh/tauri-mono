{
	"name": "tauri-mono",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "git@github.com:kavsingh/tauri-mono"
	},
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
	"scripts": {
		"dev": "turbo dev",
		"test": "turbo test",
		"test:ci": "turbo test:ci",
		"lint": "turbo lint",
		"build:mac": "turbo build:mac --filter=app",
		"build:win": "turbo build:win --filter=app",
		"clean:turbo": "rm -rf .turbo && pnpm -r exec rm -rf .turbo",
		"clean:modules": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
		"clean:types": "pnpm -r exec rm -f tsconfig.tsbuildinfo && pnpm -r exec rm -f tsconfig.ci.tsbuildinfo"
	},
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@kavsingh/eslint-plugin-filenames": "2.0.0-rc.1",
		"@tsconfig/node24": "24.0.1",
		"@tsconfig/strictest": "2.0.5",
		"@types/eslint": "9.6.1",
		"@types/node": "24.0.10",
		"eslint": "9.30.1",
		"eslint-config-prettier": "10.1.5",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-prettier": "5.5.1",
		"globals": "16.3.0",
		"prettier": "3.6.2",
		"rimraf": "6.0.1",
		"turbo": "2.5.4",
		"typescript": "5.8.3",
		"typescript-eslint": "8.35.1"
	}
}
