{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build"]
		},
		"build:mac": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build"]
		},
		"build:win": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"cache": false,
			"persistent": true
		},
		"test:ci": {
			"cache": false,
			"persistent": true
		},
		"typecheck": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"cache": false,
			"persistent": true
		}
	}
}
